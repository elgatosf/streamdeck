{
    "compilerOptions": {
        "customConditions": [
            "browser"
        ],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "dom",
            "es2022"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "strict": true,
        "target": "es2022"
    },
    "include": [
        "./"
    ]
}
